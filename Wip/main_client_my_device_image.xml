				<PlaceHolder width="20"/>
			</HorizontalLayout>
			<Layout left="0" top="33" right="0" height="2">
				<SliderIndicator name="image_client_top_sliderIndicator" backgroundimage="common_indicator_horizontal.png" sliderorign="horizontal" backgroundtransparent="false" left="0" height="2" right="0" />
			</Layout>
            <Layout left="0" top="35" right="0" height="34">
                <HorizontalLayout name="image_client_title_similar_show_layout" left="0" top="0" right="0" bottom="1" visible="true" backgroundcolor="#FCFCFC" backgroundtransparent="false">
                    <PlaceHolder width="25" />
					<Control backgroundimage="media_image_similar.png" width="16" height="16" backgroundtransparent="true"/>
					<PlaceHolder width="6" />
                    <Label name="image_client_title_similar_show_count" text="发现0张相似图片" textcolor="#262626" align="left,venter" backgroundtransparent="true" width="130" autosize="true" />
                    <Button name="image_client_title_similar_show" text="查看" align="left,venter" left="130" width="50" textcolor="#FF6A30" texthotcolor="#FF4800" textpushedcolor="#CA2B06" backgroundtransparent="true" />
                </HorizontalLayout>
                <HorizontalLayout name="image_client_title_similar_return_layout" left="0" top="0" right="0" bottom="1" visible="false" backgroundcolor="#FCFCFC" backgroundtransparent="false">
                    <PlaceHolder width="25" />
                    <Button name="image_client_title_similar_return" imagelist="media_image_similar_reutrn.png" imagepadding="0,10,37,9"
