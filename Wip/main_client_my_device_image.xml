				<PlaceHolder width="25" />
				<DecorateButton name="image_client_title_photo" optiongroup="image_client_title_group" width="100" height="35" backgroundtransparent="true">
					<Label text="相机" font="12" textcolor="#656565" textfocuscolor="#FB5A45" texthotcolor="#191919" textpushedcolor="#FB5A45" align="left,venter"
						backgroundtransparent="true" width="35" height="35" left="0" focuspriority="true"/>
					<Label name="image_client_title_photo_count" text="0" font="12" textcolor="#656565" textfocuscolor="#FB5A45" texthotcolor="#191919" textpushedcolor="#FB5A45" align="left,venter" left="30" autosize="true" height="35" backgroundtransparent="true" focuspriority="true"/>
